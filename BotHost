local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("EqiiryHub | BotHost", "Sentinel")


local Tab = Window:NewTab("Bots")
local Section = Tab:NewSection("EqiiryHub | Bots")
Section:NewToggle("EqiiryHub1", "Toggle to connect the bot to the server", function(state)
    if state then
        print("Toggle On")
