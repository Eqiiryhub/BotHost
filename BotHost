local Tab = Window:NewTab("Actions")
local Section = Tab:NewSection("EqiiryHub | Actions")

local Tab = Window:NewTab("Import")
local Section = Tab:NewSection("EqiiryHub | Import")
Section:NewTextBox("Code", "Import scripts", function(txt)
	print(txt)
end)
Section:NewButton("Execute", "Executes the code you imported", function()
    print("Clicked")
end)

local Section = Tab:NewSection("EqiiryHub | Extra")
Section:NewLabel("Created By: Eqiiry#1337")
Section:NewKeybind("KeybindText", "KeybindInfo", Enum.KeyCode.F, function()
	Library:ToggleUI()
end)
Section:NewColorPicker("Color Text", "Color Info", Color3.fromRGB(0,0,0), function(color)
    print(color)
    -- Second argument is the default color
end)
Section:NewDropdown("Version", "DropdownInf", {"1.0.0", "1.0.1", "1.0.2"}, function(currentOption)
    print(currentOption)
end)
